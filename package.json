  "scripts": {
    "compile": "tsc",
    "lint": "eslint --max-warnings=0 --ext .ts,.js src/",
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "test": "tsx src/tests/**/*.test.ts",
    "test:watch": "tsx watch src/tests/**/*.test.ts",
    "coverage": "nyc mocha -r ts-node/register src/tests/**/*.test.ts --exit && open-cli coverage/lcov-report/index.html",
    "clean": "shx rm -rf node_modules yarn.lock",
    "reinstall": "yarn clean && yarn install",
    "prepare": "husky install"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.test.ts",
      "tests/**"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "all": true
  }
}
