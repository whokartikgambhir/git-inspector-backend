{
  "name": "github-pr-analytics",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/whokartikgambhir/github-pr-analytics.git",
  "author": "whokartikgambhir <gambhirkartik6@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mongoose": "^8.15.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.15.29",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "shx": "^0.3.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "nyc": "^15.1.0",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "@types/mocha": "^10.0.1"
  },
  "scripts": {
    "compile": "shx rm -rf dist && tsc",
    "lint": "eslint --ext .ts,.js src/",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npm run compile",
    "start": "node dist/index.js",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register src/**/*.test.ts",
    "coverage": "nyc npm test",
    "clean": "shx rm -rf node_modules yarn.lock",
    "reinstall": "yarn clean && yarn install"
  }
}
